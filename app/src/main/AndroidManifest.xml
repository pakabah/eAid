<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.overcoretech.studenteaid">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.Login" />
        <activity android:name=".views.Signup" />
        <activity android:name=".views.DetailPasco" />
        <activity android:name=".views.DetailNotes" />
        <activity android:name=".views.StartPasco" />
        <activity android:name=".views.ScoreSummary" />

        <service
            android:name=".services.DataService"
            android:exported="false" />

        <activity android:name=".views.PdfRen" />
        <activity android:name=".views.Message"/>
    </application>

</manifest>